name: Third Github Actions Demo, Testing Docker Compose
run-name: ${{ github.actor }} is testing out Docker Compose with github-actions-docker-compose.yaml

on:
  push:
    branches: [ "dev" ]
  pull_request:
    branches: [ "dev" ]



